{
  "author": "Ephigenia M. Eichner",
  "name": "ikea-availability-checker",
  "description": "ikea product in-store availability checker",
  "version": "0.0.0",
  "keywords": [
    "availability",
    "ikea",
    "iows",
    "stock"
  ],
  "license": "MIT",
  "maintainer": {
    "name": "Marcel Eichner",
    "email": "marcel.eichner@ephigenia.de"
  },
  "contributors": [
    {
      "name": "Marcel Eichner",
      "email": "marcel.eichner@ephigenia.de"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Ephigenia/ikea-availability-checker.git"
  },
  "bugs": {
    "url": "https://github.com/Ephigenia/ikea-availability-checker/issues"
  },
  "main": "source/cli.js",
  "config": {
    "paths": {
      "source": "source"
    }
  },
  "scripts": {
    "changelog": "conventional-changelog -s -i CHANGELOG.md -p angular -r 0",
    "commitmsg": "conventional-changelog-lint -e",
    "lint": "eslint $npm_package_config_paths_source/**/*.js",
    "postversion": "git push && git push --tags",
    "preversion": "npm test",
    "test:ci": "mocha --reporter=dot $npm_package_config_paths_source/**/*.test.js",
    "test:watch": "mocha --watch $npm_package_config_paths_source/**/*.test.js",
    "test": "mocha \"$npm_package_config_paths_source/**/*.test.js\"",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "cli-table": "^0.3.1",
    "commander": "^2.9.0",
    "debug": "^2.6.0",
    "request": "^2.79.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^1.2.0",
    "conventional-changelog-lint": "^1.1.0",
    "eslint": "^3.14.1",
    "eslint-config-google": "^0.7.1",
    "husky": "^0.13.1",
    "mocha": "^3.2.0"
  }
}
